{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "study-nextjs-with-rtkq",
  "version": "0.1.0",
  "packageManager": "pnpm@7.26.3",
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git-cz",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "ci-test-storybook": "concurrently -k -s first -n \"SERVE,TEST\" -c \"magenta,blue\" \"pnpm http-server storybook-static --port 6006 --silent\" \"pnpm test-storybook\""
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{md,json,yml,yaml}": "prettier --write",
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --cache --quiet --fix"
    ]
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@reduxjs/toolkit": "^1.9.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@visx/react-spring": "^3.0.0",
    "@visx/responsive": "^3.0.0",
    "@visx/xychart": "^3.0.0",
    "clsx": "^1.2.1",
    "d3-scale-chromatic": "^3.0.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "isomorphic-fetch": "^3.0.0",
    "next": "13.1.6",
    "next-redux-wrapper": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "sanitize.css": "^13.0.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.4.1",
    "@jest/types": "^29.4.1",
    "@react-spring/web": "^9.6.1",
    "@storybook/addon-essentials": "7.0.0-beta.36",
    "@storybook/addon-interactions": "7.0.0-beta.36",
    "@storybook/addon-links": "7.0.0-beta.36",
    "@storybook/blocks": "7.0.0-beta.36",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/nextjs": "7.0.0-beta.36",
    "@storybook/react": "7.0.0-beta.36",
    "@storybook/test-runner": "0.10.0-next.8",
    "@storybook/testing-library": "0.0.14-next.1",
    "@storybook/testing-react": "v2.0.0-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@vanilla-extract/css": "^1.9.3",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "@vanilla-extract/webpack-plugin": "^2.2.0",
    "chromatic": "^6.15.0",
    "commitizen": "^4.3.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-testing-library": "^5.10.0",
    "fishery": "^2.2.2",
    "git-cz": "^4.9.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "msw": "^1.0.0",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.3",
    "storybook": "7.0.0-beta.36",
    "style-loader": "^3.3.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
